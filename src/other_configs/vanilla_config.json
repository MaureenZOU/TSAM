{
    "name": "VideoInpaintingModel_v0.2_l1_vgg_style_vanilla_1_1_10",
    "n_gpu": 1,
    "arch": {
        "type": "VideoInpaintingModel",
        "args": {
            "nc_in": 5,
            "nc_out": 3,
            "opts": {
                "norm": "SN",
                "nf": 90,
                "bias": true,
                "conv_type": "vanilla",
                "conv_by": "3d",
                "temporal_discriminator": false,
                "spatial_discriminator": false,
                "vgg_loss": true
            },
            "d_s_args": {
                "nf": 64,
                "use_sigmoid": true,
                "norm": "SN"
            },
            "d_t_args": {
                "nf": 128,
                "use_sigmoid": true,
                "norm": "SN"
            }
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.0001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "losses": [
        {
            "type": "ReconLoss",
            "weight": 1,
            "nickname": "loss_recon",
            "args": {
                "masked": false
            }
        },
        {
            "type": "ReconLoss",
            "weight": 0,
            "nickname": "loss_masked_recon",
            "args": {
                "masked": true
            }
        },
        {
            "type": "VGGLoss",
            "weight": 1,
            "nickname": "loss_vgg",
            "args": {}
        },
        {
            "type": "StyleLoss",
            "weight": 10,
            "nickname": "loss_style",
            "args": {}
        }
    ],
    "gan_losses": {
        "loss_gan_spatial_weight": 0,
        "loss_gan_temporal_weight": 0
    },
    "metrics": [
        "L2_loss"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 1000,
            "gamma": 0.97
        }
    },
    "trainer": {
        "epochs": 1000,
        "save_dir": "saved/",
        "save_freq": 5,
        "verbosity": 2,
        "monitor": "loss_total",
        "monitor": "test0_p_dist",
        "monitor_mode": "min",
        "mask_only": true,
        "pretrained_load_strict": false
    },
    "visualization": {
        "log_step": 40,
        "test_log_step": 400,
        "tensorboardX": true,
        "log_dir": "saved/runs"
    }
}
